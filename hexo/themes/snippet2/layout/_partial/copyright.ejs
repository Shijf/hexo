<script>

  function judgeUrl() {
    $coding_net = document.getElementById("coding_net");

    var url = window.location.href;
    var url = url.indexOf("https://www.shijf.top/");
    if(url == 0){
        $coding_net.style.display="inline";
    }else {
      $coding_net.style.display="none";

    }
  }

    judgeUrl();
